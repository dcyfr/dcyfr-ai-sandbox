{
  "name": "@dcyfr/ai-sandbox",
  "version": "1.0.0",
  "description": "Sandbox and examples for DCYFR AI framework experimentation",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=24.13.0",
    "npm": ">=11.6.2"
  },
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "example:basic": "node examples/basic-usage.js",
    "example:plugins": "node examples/plugin-system.js",
    "example:agents": "node examples/agent-patterns.js",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:tag": "changeset tag",
    "release": "changeset tag",
    "version:patch": "npm version patch -m 'chore: bump version to %s'",
    "version:minor": "npm version minor -m 'chore: bump version to %s'",
    "version:major": "npm version major -m 'chore: bump version to %s'"
  },
  "dependencies": {
    "@dcyfr/agents": "file:../dcyfr-ai-agents",
    "@dcyfr/ai": "^1.0.4"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "dcyfr",
    "ai",
    "testing",
    "examples",
    "framework"
  ],
  "author": "Drew (dcyfr)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcyfr/dcyfr-ai-sandbox.git"
  },
  "bugs": {
    "url": "https://github.com/dcyfr/dcyfr-ai-sandbox/issues"
  },
  "homepage": "https://github.com/dcyfr/dcyfr-ai-sandbox#readme"
}
